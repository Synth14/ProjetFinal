@model IEnumerable<Commande_UIL>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdCommande)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NbPlaceDemande)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalCommande)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Solvabilite)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EtatCommande)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdVoyage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdClient)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdCommande)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NbPlaceDemande)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalCommande)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Solvabilite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EtatCommande)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdVoyage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdClient)
            </td>
            <td>
                <button id="btnSolva"></button>
            </td>
        </tr>
    }

</table>
